GCC_PREPROCESSOR_DEFINITIONS = $(inherited) COCOAPODS=1
HEADER_SEARCH_PATHS = $(inherited) "${PODS_ROOT}/Headers/Public" "${PODS_ROOT}/Headers/Public/LLMPay" "${PODS_ROOT}/Headers/Public/XVIEWLLPayManager"
LIBRARY_SEARCH_PATHS = $(inherited) "${PODS_ROOT}/LLMPay/LLMPay/Core" "${PODS_ROOT}/LLMPay/LLMPay/MPay" "${PODS_ROOT}/../../XVIEWLLPayManager/Classes"
OTHER_CFLAGS = $(inherited) -isystem "${PODS_ROOT}/Headers/Public" -isystem "${PODS_ROOT}/Headers/Public/LLMPay" -isystem "${PODS_ROOT}/Headers/Public/XVIEWLLPayManager"
OTHER_LDFLAGS = $(inherited) -ObjC -l"LLMobilePay" -l"LLPayManager" -l"LLPaySDKCore" -l"ObjC"
PODS_BUILD_DIR = ${BUILD_DIR}
PODS_CONFIGURATION_BUILD_DIR = ${PODS_BUILD_DIR}/$(CONFIGURATION)$(EFFECTIVE_PLATFORM_NAME)
PODS_PODFILE_DIR_PATH = ${SRCROOT}/.
PODS_ROOT = ${SRCROOT}/Pods
